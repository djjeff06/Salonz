<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.msi.salonz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainNavigation"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <fragment android:name=".SalonListActivity"
            android:label="Salon" />
        <fragment android:name=".SalonistActivity"
            android:label="Salonist" />
        <fragment android:name=".HaircutActivity"
            android:label="Haircut" />
        <fragment android:name=".HaircutViewActivity"
            android:label="Haircut" />
        <fragment android:name=".Reserve"
            android:label="Reserve" />
    </application>

</manifest>